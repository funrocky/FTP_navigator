<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_conn_add">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="81dp"
        android:layout_weight="1"
        android:text="用户名"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/editText_connname"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="51dp"
        android:layout_marginBottom="43dp"
        android:layout_weight="1"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_conn_ip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/editText_conn_ip"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="70dp"
        android:layout_weight="1"
        android:text="IP地址"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="65dp"
        android:layout_weight="1"
        android:text="端口"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="71dp"
        android:layout_weight="1"
        android:text="密码"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="216dp"
        android:layout_weight="1"
        android:text="备注"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintEnd_toStartOf="@+id/editText_conn_info"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="54dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="60dp"
        android:layout_weight="1"
        android:text="连接名"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toStartOf="@+id/editText_connname"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText_conn_ip"
        android:layout_width="270dp"
        android:layout_height="0dp"
        android:layout_marginBottom="45dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_conn_port"
        app:layout_constraintEnd_toEndOf="@+id/editText_conn_port"
        app:layout_constraintStart_toStartOf="@+id/editText_conn_port"
        app:layout_constraintTop_toBottomOf="@+id/editText_connname" />

    <EditText
        android:id="@+id/editText_conn_port"
        android:layout_width="271dp"
        android:layout_height="0dp"
        android:layout_marginBottom="49dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="21"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_conn_username"
        app:layout_constraintEnd_toEndOf="@+id/editText_conn_username"
        app:layout_constraintStart_toStartOf="@+id/editText_conn_username"
        app:layout_constraintTop_toBottomOf="@+id/editText_conn_ip" />

    <EditText
        android:id="@+id/editText_conn_username"
        android:layout_width="266dp"
        android:layout_height="0dp"
        android:layout_marginBottom="55dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="admin"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_conn_password"
        app:layout_constraintEnd_toEndOf="@+id/editText_conn_password"
        app:layout_constraintStart_toStartOf="@+id/editText_conn_password"
        app:layout_constraintTop_toBottomOf="@+id/editText_conn_port" />

    <EditText
        android:id="@+id/editText_conn_password"
        android:layout_width="258dp"
        android:layout_height="0dp"
        android:layout_marginBottom="54dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="password"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/editText_conn_info"
        app:layout_constraintEnd_toEndOf="@+id/editText_conn_info"
        app:layout_constraintStart_toStartOf="@+id/editText_conn_info"
        app:layout_constraintTop_toBottomOf="@+id/editText_conn_username" />

    <EditText
        android:id="@+id/editText_conn_info"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="210dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/editText_conn_password" />

    <Button
        android:id="@+id/btn_conn_add_commin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="确定添加"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText_conn_info" />

</androidx.constraintlayout.widget.ConstraintLayout>

